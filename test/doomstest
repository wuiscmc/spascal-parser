programa doomstest; 
{practica PDL2}

funcion multiplica (num1: entero, f1num1: caracter, f1: real): entero
var 
	a, b: real;
	c: entero;
	multiplica: real;
	
	li: lista;
inicio
		1+1;
		a+1;
		1;
fin;
var 
	a, b, num1 : real;
	num2: caracter;
	c : entero;
inicio
	
	repite 
		escribe('cadena', num1, 'cadena',num1,a);
		multiplica(1,num2,c);
		si b = c entonces 
			escribe('1')
		sino 
			inicio
				b+1;
				li+1;
				c+b;
				li<<1;
				li?;
				li?(1);
				li?(b);
				a?;
				a?;
				li<<li;
			fin
		;
		
	hasta c > num2;

fin.