programa doomstest; 
{practica PDL2}

const
	PDL=2123123;
	AC=VERDADERO;
	BC=FALSO;

tipo 
	eso = lista;

funcion multiplica (num1: entero, f1num1: caracter, f1: real): entero,
var
	a, b: real;
	funcion multiplica (num1: entero, f1num1: caracter, f1: real): entero,
	var 
		
		a: eso;
		b:real;
	inicio
		BC;
		si AC entonces
			si a entonces
				PDL + a
		;
	fin;
				
inicio
	BC;
	AC;
fin;

var 
	a, b, num1 : real;
	num2: entero;
	c : caracter;
	pdl: eso;
	pdl2: lista;
inicio
	repite
		si AC entonces
			si PDL = 2 entonces
				PDL
		;
		PDL + a;
		multiplica(1,c,a);
	hasta pdl > VERDADERO;

	PDL + pdl2;
	pdl?(c);
	pdl2?(c);
	pdl2<<c;
	b + PDL;

fin.